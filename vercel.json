{
  "rewrites": [
    {
      "source": "/app",
      "destination": "https://astromic-app.vercel.app/"
    },
    {
      "source": "/app/:path*",
      "destination": "https://astromic-app.vercel.app/:path*"
    },
    {
      "source": "/_next/:path*",
      "destination": "https://astromic-app.vercel.app/_next/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "https://astromic-app.vercel.app/assets/:path*"
    }
  ]
}